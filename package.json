{
  "name": "@eluvio/elv-client-js",
  "version": "0.2.13",
  "description": "Javascript client for the  Eluvio Content Fabric",
  "main": "src/ElvClient.js",
  "author": "Kevin Talmadge",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-client-js",
  "scripts": {
    "build": "npm run build-contracts && npm run build-docs && npm run build-frame-client && npm run build-dist && npm run build-prod",
    "build-prod": "npm run build-web && npm run build-node",
    "build-dev": "npm run build-web-dev && npm run build-node-dev",
    "build-web": "webpack --progress --mode production --target web --output-filename ElvClient-min.js --devtool none",
    "build-web-dev": "webpack --progress --target web --output-filename ElvClient-min-dev.js",
    "build-node": "webpack --progress --mode production --target node --output-filename ElvClient-node-min.js --devtool none",
    "build-node-dev": "webpack --progress --target node --output-filename ElvClient-node-min-dev.js",
    "build-dist": "node ./node_modules/@babel/cli/bin/babel.js --presets=@babel/preset-env src --out-dir dist/src",
    "build-frame-client": "webpack -p --progress --entry ./dist/src/FrameClient.js --output-filename ElvFrameClient-min.js --devtool none --optimize-minimize",
    "build-contracts": "node build/BuildContracts.js",
    "build-docs": "./node_modules/.bin/jsdoc --configure ./.jsdoc.json --readme README.md",
    "add-build": "git add dist && git add src/contracts && git add src/events && git add docs",
    "lint": "./node_modules/.bin/eslint \"src/**/*.js\" \"test/**/*.js\" --fix",
    "lint-docs": "./node_modules/.bin/eslint \"vendor/elv-jsdoc-template/tmpl/*.tmpl\" \"vendor/elv-jsdoc-template/**/*.js\" --fix",
    "publish": "npm publish --access public",
    "test": "jest",
    "test-cov": "GENERATE_METHOD_LOGS=true jest --coverage --coverageDirectory 'test/coverage'",
    "test-update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "pre-commit": {
    "run": [
      "lint",
      "build",
      "add-build"
    ]
  },
  "browser": {
    "fs": false
  },
  "files": [
    "/build",
    "/dist",
    "/contracts",
    "/setup",
    "/src",
    "LICENSE",
    "README.md",
    "package.json",
    "package-lock.json",
    "webpack.config.js"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@eluvio/crypto": "^1.0.6",
    "babel-loader": "^8.0.6",
    "bignumber.js": "^8.0.2",
    "bs58": "^4.0.1",
    "buffer": "^5.2.1",
    "cbor": "^4.1.5",
    "crypto": "git+https://github.com/tradle/react-native-crypto.git",
    "deep-equal": "^1.0.1",
    "ethers": "^4.0.26",
    "hash.js": "^1.1.7",
    "js-polyfills": "^0.1.42",
    "mime-types": "^2.1.24",
    "multicodec": "^0.5.3",
    "multihashes": "^0.4.14",
    "node-fetch": "^2.3.0",
    "node-fetch-polyfill": "^2.0.6",
    "path": "^0.12.7",
    "sjcl": "^1.0.8",
    "typedarray": "0.0.6",
    "unorm": "^1.5.0",
    "urijs": "^1.19.1",
    "url-join": "^4.0.0",
    "varint": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "copy-webpack-plugin": "^4.5.2",
    "core-js": "^3.1.4",
    "eslint": "^5.15.0",
    "eslint-plugin-html": "^5.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jsdoc": "^3.6.3",
    "lodash.isequal": "^4.5.0",
    "minami": "^1.2.3",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "shell-quote": "^1.7.2",
    "solc": "0.4.24",
    "truffle": "^4.1.14",
    "truffle-contract": "^3.0.7",
    "truffle-test-utils": "^0.1.0",
    "uuid": "^3.3.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "yargs": "^14.2.0"
  }
}
