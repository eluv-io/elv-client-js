{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src"],
    "exclude": ["src/marketplaceClient"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "./vendor/elv-jsdoc-template/jsdoc-namedparams-plugin.js"
  ],
  "templates": {
    "referenceTitle": "Eluvio Javascript Client",
    "disableSort": false,
    "collapse": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "pedantic": true,
    "private": true,
    "recurse": true,
    "template": "vendor/elv-jsdoc-template"
  }
}
